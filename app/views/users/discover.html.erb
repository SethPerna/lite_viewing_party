<div class="top-rated-movies">
  <%= button_to "Top Rated Movies", "/users/#{@user.id}/discover", method: :get, params: {top_rated: 'top_rated'}  %>
  <% if @top_movies %>
  <ol>
    <% @top_movies.each do |movie| %>
      <li><%= movie.title %></li>
    <% end %>
  </ol>
  <% end %>
</div>
<divi class= "search-movie">
  <%= form_with url: search_path, local: true do |form| %>
    <%= form.label :search, 'Search For Movie by Name' %>
    <%= form.text_field :search %>
    <%= form.submit 'Search' %>
  <% end %>
  <% if @movie %>
    <h5>Movie <%=  %></h5>
  <% end %>
</div>
